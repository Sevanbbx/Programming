a = int(input('a = '))
b = int(input('b = '))
if a > b:
    a = a-(a-b)
    b = b+(a-b)
    print('a =', a)
    print('b =', b)
elif a < b:
    a = a+(b-a)
    b = b-(b-a)
    print('a =', a)
    print('b =', b)
else:
    print('Թվերը պետք է հավասար չլինեն')



#2rd xdnir

n = int(input('n = '))
a = n//1000
b = (n//100)%10
c = (n%100)//10
d = n%10
print(a,b,c,d)
