n = int(input('Input the number of elements\n'))
a = []
diagonal_1 = []
diagonal_2 = []
for i in range(n):
    a.append([])
    for j in range(n):
        j = int(input())
        a[i].append(j)
for i in a:
    print(i)
b = 0
c = 0
for i in a:
    diagonal_1.append(a[b][c])
    b+=1
    c+=1
print('-----------------------------')
q=-1
for i in a:
        diagonal_2.append(i[q])
        q-=1
        
print(diagonal_1)
print(diagonal_2)
